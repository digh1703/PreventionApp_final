<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.preventionapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".BoardContentsList"></activity>
        <activity android:name=".AppInfo" />
        <activity android:name=".AccessActivity" />
        <activity android:name=".CareMenuFragment" />


        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="0l8htuamay" />

        <activity android:name=".DropdownAdapter" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <meta-data android:name="android.app.searchable"
                android:resource="@xml/searchable"/>
        </activity>
        <activity android:name=".MessageActivity"/>
        <activity android:name=".CrimeMap_Robbery" />
        <activity android:name=".CrimeMap_Murder" />
        <activity android:name=".CrimeMap_Rape" />
        <activity android:name=".CrimeMap_Larceny" />
        <activity android:name=".CrimeMap_Violence" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".SearchActivity"/>
        <activity android:name=".BoardContentsActivity" />
        <activity android:name=".BoardContentsWriteActivity" />
        <activity android:name=".myGroup" />
        <activity android:name=".PreventionInfoContentsActivity" />
        <activity android:name=".ETFSlideActivity" />
    </application>

</manifest>